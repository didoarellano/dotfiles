alias mxs="tmuxinator start"
alias mxo="tmuxinator open"
alias mxc="tmuxinator copy"
alias mxD="tmuxinator delete"
alias mxi="tmuxinator implode"
alias mxl="tmuxinator list"
alias mxL"tmuxinator list -v"
alias mxd="tmuxinator doctor"

